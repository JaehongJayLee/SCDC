         "@schedule": {"interval": 20, "duration": 40, "opportunistic": true, "strict": true}
        },
        {"@type": "edu.mit.media.funf.probe.builtin.TimeOffsetProbe",
         "@schedule": {"interval": 20, "opportunistic": true, "strict": true}
